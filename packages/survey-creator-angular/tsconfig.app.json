/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./dist/angular-ui",
    "baseUrl": "./",
    "skipLibCheck": true,
    "paths": {
      "survey-creator-angular": [
        "./build/survey-creator-angular"
      ],
      "survey-core": [
        "./node_modules/survey-core"
      ]
    }
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}